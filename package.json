  },
  "bugs": {
    "url": "https://github.com/WulfTheGod/jwt-forge/issues"
  },
  "homepage": "https://github.com/WulfTheGod/jwt-forge#readme",
  "license": "MIT",
  "dependencies": {
    "jose": "^5.2.0",
    "inquirer": "^9.2.15",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
